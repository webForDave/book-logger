<h1>All Books</h1>
<a href="/">Home</a> | <a href="/books/new">New</a>
<% if (books.length == 0){ %>
    <p>Ooops, sorry, no books available yet</p>
<% } else { %>
    <% books.forEach(book => { %>
        <div>
            <p><%= book.title%> - <small><%= book.author %></small> </p>
        </div>
   <% }) %>
<% } %>